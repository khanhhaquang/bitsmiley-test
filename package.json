{
  "name": "bitsmiley-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "convertImg:dev": "node scripts/convertImage.js development",
    "convertImg:pro": "node scripts/convertImage.js production",
    "generateImgPaths": "node scripts/generateResourcesPaths.js",
    "prebuild:dev": "npm run convertImg:dev && tsc && vite build --mode development",
    "prebuild:pro": "npm run convertImg:pro && tsc && vite build --mode production",
    "build:dev": "npm run prebuild:dev && npm run generateImgPaths && npm run prebuild:dev",
    "build:pro": "npm run prebuild:pro && npm run generateImgPaths && npm run prebuild:pro",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "analyze": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-query-devtools": "^5.24.0",
    "axios": "^1.6.3",
    "axios-retry": "^4.0.0",
    "classnames": "^2.4.0",
    "fontfaceobserver": "^2.3.0",
    "ismobilejs": "^1.1.1",
    "ordpool-parser": "^0.0.6-beta.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-tooltip": "^5.26.0",
    "tailwind-merge": "^2.2.0",
    "viem": "^2.7.13",
    "wagmi": "^2.5.7"
  },
  "devDependencies": {
    "@types/fontfaceobserver": "^2.1.3",
    "@types/node": "^20.11.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.32",
    "prettier": "3.1.1",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-svgr": "^4.2.0"
  }
}
