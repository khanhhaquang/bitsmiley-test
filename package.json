{
  "name": "bitsmiley-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "postinstall": "simple-git-hooks",
    "dev": "vite",
    "generateImgPaths": "node scripts/generateResourcesPaths.js",
    "prebuild:dev": "tsc && vite build --mode development",
    "prebuild:pro": "tsc && vite build --mode production",
    "build:dev": "npm run prebuild:dev && npm run generateImgPaths && npm run prebuild:dev",
    "build:pro": "npm run prebuild:pro && npm run generateImgPaths && npm run prebuild:pro",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "analyze": "npx vite-bundle-visualizer",
    "generateContractHooks": "npx wagmi generate"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint"
  },
  "dependencies": {
    "@particle-network/btc-connectkit": "^1.0.0-alpha.26",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.0.1",
    "@sats-connect/core": "^0.2.2",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-query-devtools": "^5.24.0",
    "@types/lodash": "^4.17.0",
    "axios": "^1.6.3",
    "axios-retry": "^4.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "fontfaceobserver": "^2.3.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-imask": "^7.5.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-tooltip": "^5.26.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.9.15",
    "wagmi": "^2.5.19"
  },
  "devDependencies": {
    "@types/fontfaceobserver": "^2.1.3",
    "@types/node": "^20.11.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@wagmi/cli": "^2.1.1",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.32",
    "prettier": "3.1.1",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.10.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.2",
    "vite": "^5.4.6",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  }
}
